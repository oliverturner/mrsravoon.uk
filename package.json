  "homepage": "https://www.ravoon.uk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliverturner/ravoon.uk"
  },
  "author": {
    "name": "Oliver Turner",
    "email": "oliver.turner@codedsignal.co.uk",
    "url": "https://www.codedsignal.co.uk"
  },
  "bugs": {
    "url": "https://github.com/oliverturner/ravoon.uk/issues"
  },
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.1.5",
    "autoprefixer": "9.8.6",
    "clean-css": "^4.2.1",
    "html-minifier": "^4.0.0",
    "postcss": "7.0.32",
    "sass": "1.26.10",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.0",
    "@babel/core": "7.11.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-typescript": "5.0.2",
    "@tsconfig/svelte": "1.0.8",
    "@types/luxon": "^1.21.0",
    "@types/markdown-it": "10.0.1",
    "@types/markdown-it-anchor": "^4.0.3",
    "@types/markdown-it-container": "^2.0.2",
    "@types/node": "14.0.27",
    "@types/serve-static": "1.13.5",
    "babel-eslint": "10.1.0",
    "eslint": "7.6.0",
    "eslint-plugin-jest": "23.20.0",
    "husky": "4.2.5",
    "jest": "26.2.2",
    "lint-staged": "10.2.11",
    "luxon": "^1.22.2",
    "markdown-it": "11.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-div": "^1.1.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "0.10.0",
    "markdown-it-sup": "^1.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "rollup": "2.23.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-postcss": "3.1.4",
    "rollup-plugin-svelte": "5.2.3",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-selector-bem-pattern": "2.1.0",
    "svelte": "3.24.1",
    "svelte-check": "1.0.10",
    "svelte-preprocess": "4.0.10",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run lint:js -- --fix"
    ],
    "*.css": [
      "npm run prettier",
      "npm run lint:css"
    ],
    "*.scss": [
      "npm run prettier",
      "npm run lint:css"
    ]
  },
  "scripts": {
    "rollup": "rollup",
    "clean": "git clean -fxdi",
    "prettier": "prettier --write '**/*.{js,json,css,scss,ts}'",
    "test": "jest",
    "lint:css": "stylelint ./client/styles/**/*.scss",
    "lint:js": "eslint ./tools/**/*.js ./client/js/**/*.js",
    "lint:ts": "tsc",
    "lint": "run-s lint:*",
    "build": "NODE_ENV=production npx eleventy",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "dev:rollup": "npm run rollup -- -w",
    "dev:eleventy": "npx eleventy --serve",
    "dev": "run-p dev:*"
  }
}
